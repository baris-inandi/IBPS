sub biggestRow(MATRIX)
    MAX = 0
    MAX_IDX = -1
    loop I from 0 to MATRIX.length - 1
        SUM = 0
        loop J from 0 to MATRIX[I].length - 1
            ELEM = MATRIX[I][J]
            SUM += ELEM
        end loop
        if SUM >= MAX then
            MAX = SUM
            MAX_IDX = I
        end if
    end loop
    return MAX_IDX
end sub

TEST = [
    [1,2,5,6],
    [8,9,4,10,11],
    [1,2],
    [5,6,3],
    [6,5,3,6,8,2],
    [1,8,91,2,2]
]

output biggestRow(TEST)