{
    "name": "ibps-ide",
    "version": "0.7.8",
    "license": "AGPL-3.0-only",
    "author": {
        "name": "baris-inandi",
        "url": "https://inandioglu.com"
    },
    "scripts": {
        "dev": "rsbuild dev",
        "dev:tauri": "tauri dev",
        "build": "rsbuild build",
        "build:analyze": "BUNDLE_ANALYZE=true bun run build",
        "build:tauri": "tauri build",
        "build:all": "bun run wasm-prod && bun run build",
        "icon": "tauri icon icons/appicon.png",
        "preview": "rsbuild preview",
        "prettier": "prettier --write .",
        "wasm-dev": "cd ibpscomp-rs && wasm-pack build . --target web --dev && cd .. && bun add ./ibpscomp-rs/pkg",
        "wasm-prod": "cd ibpscomp-rs && wasm-pack build . --target web --release && cd .. && bun add ./ibpscomp-rs/pkg",
        "wasm-hard-reset": "rm -rf ibpscomp-rs/target && rm -rf ibpscomp-rs/pkg && bun remove ibpscomp-rs && bun wasm-prod"
    },
    "dependencies": {
        "@tauri-apps/api": "^1.5.3",
        "@vercel/analytics": "^1.1.1",
        "@vercel/speed-insights": "^1.0.1",
        "ibpscomp-rs": "./ibpscomp-rs/pkg",
        "jotai": "^2.4.3",
        "lz-string": "^1.5.0",
        "preact": "^10.19.4",
        "pretty-bytes": "^6.1.1",
        "react-ace": "^10.1.0",
        "react-icons": "^4.11.0",
        "react-py": "^1.10.6",
        "react-resizable-panels": "^0.0.55",
        "typescript": "5.1.6",
        "use-prefers-color-scheme": "^1.1.3",
        "wouter-preact": "^3.0.0"
    },
    "devDependencies": {
        "@rsbuild/core": "^0.4.1",
        "@rsbuild/plugin-preact": "^0.4.2",
        "@tauri-apps/cli": "^1.5.9",
        "@tsconfig/strictest": "^2.0.2",
        "autoprefixer": "^10.4.17",
        "postcss": "^8.4.35",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.5.11",
        "tailwindcss": "^3.4.1",
        "wasm-pack": "^0.12.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
